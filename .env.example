# Telegram Bot Token (получить от @BotFather)
BOT_TOKEN=your_telegram_bot_token_here

# OpenAI API Key (получить на platform.openai.com)
OPENAI_API_KEY=your_openai_api_key_here

# Supabase (для хранения данных пользователей)
# Получить на supabase.com -> Project Settings -> API
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key_here

# Admin User ID (опционально, для команды /stats)
# Получить свой Telegram ID можно через бота @userinfobot
ADMIN_USER_ID=your_telegram_user_id_here
